      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">uima.tcas.DocumentAnnotation</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">org.ohnlp.medtagger.type.ConceptMention</type>
        </outputs>
        <languagesSupported/>
